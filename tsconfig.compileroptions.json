{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "lib": ["es2019"],
    "module": "commonjs",
    "target": "es2019",
    "allowJs": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "composite": true
  }
}
